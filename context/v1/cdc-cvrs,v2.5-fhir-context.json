{
    "@context": {
        "@version": 1.1,
        "@protected": true,
        "id": "@id",
        "type": "@type",

        "VaccinationCertificate": {
            "@id": "https://w3id.org/vaccination#VaccinationCertificate",

            "@context": {
                "@version": 1.1,
                "@protected": true,

                "id": "@id",
                "type": "@type",

                "description": "http://schema.org/description",
                "identifier": "http://schema.org/identifier",
                "name": "http://schema.org/name"
            }
        },

        "VaccinationEvent": {
            "@id":   "https://w3id.org/vaccination#VaccineRecipient",

            "@context": {
                "@version": 1.1,
                "@protected": true,

                "id": "@id",
                "type": "@type",

                "administeringCentre": {
                    "@id": "http://hl7.org/fhir/Immunization.location.name",
                    "@type": "http://www.w3.org/2001/XMLSchema#string"
                },
                "batchNumber": {
                    "@id":"http://hl7.org/fhir/Immunization.lotNumber",
                    "@type": "http://www.w3.org/2001/XMLSchema#string"
                },
                "dateOfVaccination": {
                    "@id":"http://hl7.org/fhir/Immunization.occurenceDateTime", 
                    "@type": "http://www.w3.org/2001/XMLSchema#dateTime"
                },
                "recipient": {
                    "@id": "https://w3id.org/vaccination#recipient",
                    "@type": "https://w3id.org/vaccination#VaccineRecipient"
                },
                "vaccine": {
                    "@id": "http://w3id.org/vaccination#cdc-cvrs-eua-vaccine-code",
                    "@type": "http://www.w3.org/2001/XMLSchema#integer"
                }
            }
        },

        "VaccineRecipient": {
            "@id": "https://w3id.org/vaccination#VaccineRecipient",
            "@type": "http://hl7.org/fhir/Patient",

            "@context": {
                "@version": 1.1,
                "@protected": true,

                "id": "@id",
                "type": "@type",

                "birthDate": {
                    "@id": "http://hl7.org/fhir/Patient.birthDate", 
                    "@type": "http://www.w3.org/2001/XMLSchema#dateTime"
                },
                "familyName": { 
                    "@id": "http://hl7.org/fhir/Patient.name.familyName",
                    "@type": "http://www.w3.org/2001/XMLSchema#string"
                },
                "gender": { 
                    "@id": "http://hl7.org/fhir/Patient.name.gender",
                    "@type": "http://www.w3.org/2001/XMLSchema#string"
                },
                "givenName": { 
                    "@id": "http://hl7.org/fhir/Patient.name.givenName",
                    "@type": "http://www.w3.org/2001/XMLSchema#string"
                }
            }
        },


        "Vaccine": {
            "@id": "https://w3id.org/vaccination#Vaccine",

            "@context": {
                "@version": 1.1,
                "@protected": true,

                "id": "@id",
                "type": "@type",

                "atcCode": {
                    "@id": "http://w3id.org/vaccination#covid19-cdc-cvrs-vaccine-code",
                    "@type": "http://www.w3.org/2001/XMLSchema#string"
                },
                "event": {
                    "@id": "https://w3id.org/vaccination#VaccineRecipientVaccineEvent",
                    "@type": "https://w3id.org/vaccination#VaccineEvent"
                }
            }
        }
    }
}